{
  "name": "gateway",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec \"ts-node src/index.ts\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.0.0",
    "@sentry/node": "^10.20.0",
    "@supabase/supabase-js": "^2.76.1",
    "argon2": "^0.40.1",
    "axios": "^1.7.7",
    "bullmq": "^5.61.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "google-auth-library": "^10.4.1",
    "googleapis": "^133.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.10.1",
    "pino": "^9.0.0",
    "pino-pretty": "^11.2.1",
    "socket.io": "^4.8.1",
    "stripe": "^14.25.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.2",
    "nodemon": "^3.0.2",
    "prisma": "^6.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
